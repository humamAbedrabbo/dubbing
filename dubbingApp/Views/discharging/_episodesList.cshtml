@model IEnumerable<dubbingModel.orderTrnHdr>

<div class="container-fluid" style="padding:0px">
    @foreach(var w in Model.Select(b => b.agreementWork).Distinct())
    {
        <label>@w.workName</label>
        <br />
        <ul>
            @foreach (var oi in Model.Where(b => b.workIntno == w.workIntno))
                {
                    string id = "oi" + oi.orderTrnHdrIntno;
                <li>
                    <small><a href="#" class="episodeLbl" id="'@id'" onclick="onEpisodeSelectClick(@oi.orderTrnHdrIntno)">Episode: &nbsp; @oi.episodeNo</a></small>
                </li>
            }
        </ul>
        <hr />
    }
    
</div>